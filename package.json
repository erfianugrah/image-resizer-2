{
	"name": "image-resizer-2",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"deploy:dev": "wrangler deploy --env development",
		"deploy:staging": "wrangler deploy --env staging",
		"deploy:prod": "wrangler deploy --env production",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "eslint src/**/*.ts",
		"typecheck": "tsc --noEmit",
		"build": "node scripts/generate-browser-support.js && npx esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js",
		"cf-typegen": "wrangler types",
		"update-browser-support": "node scripts/generate-browser-support.js",
		"config:post": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts post",
		"config:get": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts get",
		"config:load-kv": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts load-kv",
		"config:activate": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts activate",
		"config:init-env": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts init-env",
		"config:load-initial": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts load-kv ./docs/public/configuration/examples/templates/initial-config.json --env dev",
		"config:load-production": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts load-kv ./docs/public/configuration/examples/templates/production-config.json --env production",
		"config:modules": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules",
		"config:modules:list": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules list",
		"config:modules:get": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules get",
		"config:modules:validate": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules validate",
		"config:modules:upload-kv": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules upload-kv",
		"config:comprehensive": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts comprehensive",
		"config:comprehensive:create": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts comprehensive create",
		"config:comprehensive:extract": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts comprehensive extract",
		"config:fix-storage": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/config-loader.ts modules upload-kv storage --env dev"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.7.5",
		"@cloudflare/workers-types": "^4.20250406.0",
		"@mdn/browser-compat-data": "^5.7.5",
		"@types/node": "^20.11.30",
		"@types/node-fetch": "^2.6.11",
		"@typescript-eslint/eslint-plugin": "^8.28.0",
		"@typescript-eslint/parser": "^8.28.0",
		"caniuse-db": "^1.0.30001706",
		"chalk": "^4.1.2",
		"commander": "^12.0.0",
		"dotenv": "^16.4.5",
		"esbuild": "^0.25.1",
		"eslint": "^8.57.0",
		"node-fetch": "^2.7.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2",
		"vitest": "~3.0.7",
		"wrangler": "^4.7.2"
	},
	"dependencies": {
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0"
	}
}
